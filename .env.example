# Alpaca API Credentials
# Get your keys from: https://app.alpaca.markets/paper/dashboard/overview
# Copy this file to .env and fill in your actual values

# NOTE: Trading API keys are SEPARATE from data API keys!

# Trading API (for account, positions, orders)
export ALPACA_BASE_URL="https://paper-api.alpaca.markets"
export ALPACA_API_KEY="your_trading_api_key_here"
export ALPACA_API_SECRET="your_trading_api_secret_here"

# Data API (for historical bars, snapshots, quotes)
export ALPACA_DATA_URL="https://data.alpaca.markets"
export ALPACA_DATA_API_KEY="your_data_api_key_here"
export ALPACA_DATA_API_SECRET="your_data_api_secret_here"
